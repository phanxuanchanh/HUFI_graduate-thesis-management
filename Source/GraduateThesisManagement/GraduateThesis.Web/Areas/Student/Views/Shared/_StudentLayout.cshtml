
<!DOCTYPE html>
<html lang="en">
<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	@await Component.InvokeAsync("StudentLayout_HeadElement")
	<!-- base:css -->
	<link rel="stylesheet" href="~/kapella-free/template/vendors/mdi/css/materialdesignicons.min.css">
	<link rel="stylesheet" href="~/kapella-free/template/vendors/base/vendor.bundle.base.css">
	<!-- endinject -->
	<!-- plugin css for this page -->
	<!-- End plugin css for this page -->
	<!-- inject:css -->
	<link rel="stylesheet" href="~/kapella-free/template/css/style.css">
	<!-- endinject -->
    <link rel="shortcut icon" href="~/kapella-free/template/images/favicon.png" />
    <link href="~/student-area/student-thesis.css" rel="stylesheet" />
</head>
<body>
	<div class="container-scroller">
		<!-- partial:partials/_horizontal-navbar.html -->
		<div class="horizontal-menu">
			@await Component.InvokeAsync("TopNavbar")
			@await Component.InvokeAsync("BottomNavbar")
		</div>
		<!-- partial -->
		<div class="container-fluid page-body-wrapper">
			<div class="main-panel">
				<div class="content-wrapper">
					@RenderBody()
				</div>
				<!-- content-wrapper ends -->
				<!-- partial:partials/_footer.html -->
				<footer class="footer">
					<div class="footer-wrap">
						<div class="d-sm-flex justify-content-center justify-content-sm-between">
							<span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © <a href="https://hufi.edu.vn" target="_blank">hufi.edu.vn </a>@DateTime.Now</span>
							<span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Nhóm sinh viên thực hiện đề tài <a href="# target="_blank">#</a></span>
						</div>
					</div>
				</footer>
				<!-- partial -->
			</div>
			<!-- main-panel ends -->
		</div>
		<!-- page-body-wrapper ends -->
	</div>
	<!-- container-scroller -->
	<!-- base:js -->
	<script src="vendors/base/vendor.bundle.base.js"></script>
	<!-- endinject -->
	<!-- Plugin js for this page-->
	<!-- End plugin js for this page-->
	<!-- inject:js -->
	<script src="js/template.js"></script>
	<!-- endinject -->
	<!-- plugin js for this page -->
	<!-- End plugin js for this page -->
	<script src="~/kapella-free/template/vendors/chart.js/Chart.min.js"></script>
	<script src="~/kapella-free/template/vendors/progressbar.js/progressbar.min.js"></script>
	<script src="~/kapella-free/template/vendors/chartjs-plugin-datalabels/chartjs-plugin-datalabels.js"></script>
	<script src="~/kapella-free/template/vendors/justgage/raphael-2.1.4.min.js"></script>
	<script src="~/kapella-free/template/vendors/justgage/justgage.js"></script>
	<script src="~/kapella-free/template/js/jquery.cookie.js" type="text/javascript"></script>
	<!-- Custom js for this page-->
	<script src="~/kapella-free/template/js/dashboard.js"></script>
	<!-- End custom js for this page-->
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>